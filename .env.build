# Environment variables for Docker build
# These variables help prevent AWS SDK issues during build

# Disable AWS SDK configuration loading during build
AWS_SDK_LOAD_CONFIG=0

# Set minimal AWS configuration to prevent service discovery
AWS_REGION=eu-west-2

# Disable Next.js telemetry
NEXT_TELEMETRY_DISABLED=1

# Disable Node.js warnings during build
NODE_NO_WARNINGS=1

# Set build environment
NODE_ENV=production

# Prevent Cloud9 and other AWS service checks
AWS_SDK_JS_SUPPRESS_MAINTENANCE_MODE_MESSAGE=1
